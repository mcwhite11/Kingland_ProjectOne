@{
    //Grab our username
    var user = Session["user"];
	
	//Set/grab variables
    var first = Request.Form["first"];
    var last = Request.Form["last"];
    var hPhone = Request.Form["hPhone"];
    var cPhone = Request.Form["cPhone"];
    var ICEName = Request.Form["ICEName"];
    var ICEHPhone = Request.Form["ICEHPhone"];
    var ICECPhone = Request.Form["ICECPhone"];
    var ICERel = Request.Form["ICERel"];
    var ethnicity = Request.Form["ethnicity"];
    var disabled = Request.Form["disabled"];
    var disAcom = Request.Form["disAcom"];
    var acomExp = Request.Form["vetacomExp"];
    var vet = Request.Form["vet"];
    var disVet = Request.Form["disVet"];
    var noVet = Request.Form["noVet"];

    //Connnect to database
    var db = Database.Open("kingland");

    //Update users table
    db.QuerySingle("UPDATE users SET first = '@0', last = '@1' WHERE user = '@2'", first, last, user);

    //Update users table
    db.QuerySingle("UPDATE addInfo SET hPhone = '@0', cPhone = '@1', ICEName = '@2', ICEHPhone = '@3', ICECPhone = '@4', ICERel = '@5', ethnicity = '@6', disabled = '@7', disAcom = '@8', acomExp = '@9', vet = '@10', disVet = '@11', noVet = '@12', isDone = '1' WHERE user = '@13'", hPhone, cPhone, ICEName, ICEPhone, ICERel, ethnicity, disabled, acomExp, vet, disVet, noVet, user);
	
	//Head to the next form
	Response.redirect("../start.cshtml?form=2"); 
}

