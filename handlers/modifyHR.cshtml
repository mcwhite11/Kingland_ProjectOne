@{
    //Grab our username
    var user = Session["user"];
    
    var first = Request.Form["first"];
    var last = Request.Form["last"];
    var email = Request.Form["email"];
    var hmemail = Request.Form["hmemail"];
    var prefName = Request.Form["prefName"];
    var position = Request.Form["position"];
    var dept = Request.Form["dept"];
    var actSD = Request.Form["actSD"];
    var hm = Request.Form["hm"];
    
     //Update users table
    db.QuerySingle("UPDATE users SET first = '@0', last = '@1', email = '@2', hmemail= '@3', prefName = '@4', position = '@5', dept = '@6', startDate = '@7', hmName = '@8' WHERE user = '@9'", first, last, email, hmemail, prefName, position, dept, actSD, hm, user);
    
    //Head to message
	Response.redirect("../cpanel.php?page=formSuccess"); 
} 

